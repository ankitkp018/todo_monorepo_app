{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "todo-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/todo-app",
      "sourceRoot": "projects/todo-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/todo-app",
            "index": "projects/todo-app/src/index.html",
            "main": "projects/todo-app/src/main.ts",
            "polyfills": "projects/todo-app/src/polyfills.ts",
            "tsConfig": "projects/todo-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/todo-app/src/favicon.ico",
              "projects/todo-app/src/assets"
            ],
            "styles": [
              "projects/todo-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/todo-app/src/environments/environment.ts",
                  "with": "projects/todo-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "todo-app:build:production"
            },
            "development": {
              "browserTarget": "todo-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/todo-app/src/test.ts",
            "polyfills": "projects/todo-app/src/polyfills.ts",
            "tsConfig": "projects/todo-app/tsconfig.spec.json",
            "karmaConfig": "projects/todo-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/todo-app/src/favicon.ico",
              "projects/todo-app/src/assets"
            ],
            "styles": [
              "projects/todo-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "projects/ui",
      "sourceRoot": "projects/ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ui/src/test.ts",
            "tsConfig": "projects/ui/tsconfig.spec.json",
            "karmaConfig": "projects/ui/karma.conf.js"
          }
        }
      }
    },
    "data-access": {
      "projectType": "library",
      "root": "projects/data-access",
      "sourceRoot": "projects/data-access/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/data-access/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/data-access/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/data-access/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/data-access/src/test.ts",
            "tsConfig": "projects/data-access/tsconfig.spec.json",
            "karmaConfig": "projects/data-access/karma.conf.js"
          }
        }
      }
    },
    "core": {
      "projectType": "library",
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js"
          }
        }
      }
    },
    "login-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/login-app",
      "sourceRoot": "projects/login-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/login-app",
            "index": "projects/login-app/src/index.html",
            "main": "projects/login-app/src/main.ts",
            "polyfills": "projects/login-app/src/polyfills.ts",
            "tsConfig": "projects/login-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/login-app/src/favicon.ico",
              "projects/login-app/src/assets"
            ],
            "styles": [
              "projects/login-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/login-app/src/environments/environment.ts",
                  "with": "projects/login-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "login-app:build:production"
            },
            "development": {
              "browserTarget": "login-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "login-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/login-app/src/test.ts",
            "polyfills": "projects/login-app/src/polyfills.ts",
            "tsConfig": "projects/login-app/tsconfig.spec.json",
            "karmaConfig": "projects/login-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/login-app/src/favicon.ico",
              "projects/login-app/src/assets"
            ],
            "styles": [
              "projects/login-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
